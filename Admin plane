-- üé® Theme Buttons
local function CreateThemeButton(name, position, theme)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0, 30, 0, 30)
    btn.Position = position
    btn.BackgroundColor3 = theme.Accent
    btn.BorderSizePixel = 0
    btn.Text = ""
    btn.Parent = TopBar
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(1, 0)
    corner.Parent = btn
    
    btn.MouseButton1Click:Connect(function()
        CurrentTheme = theme
        MainFrame.BackgroundColor3 = theme.Primary
        TopBar.BackgroundColor3 = theme.Secondary
        TopBarBottom.BackgroundColor3 = theme.Secondary
        Glow.ImageColor3 = theme.Glow
        -- Update all UI elements with new theme
    end)
    
    return btn
end

CreateThemeButton("Dark", UDim2.new(1, -120, 0.5, -15), Themes.Dark)
CreateThemeButton("Neon", UDim2.new(1, -80, 0.5, -15), Themes.Neon)
CreateThemeButton("Galaxy", UDim2.new(1, -40, 0.5, -15), Themes.Galaxy)

