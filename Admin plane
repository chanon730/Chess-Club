-- üé® Create Category Button
local function CreateCategoryButton(categoryName, icon, index)
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(1, -10, 0, 50)
    button.Position = UDim2.new(0, 5, 0, (index - 1) * 55)
    button.BackgroundColor3 = CurrentTheme.Primary
    button.BorderSizePixel = 0
    button.Text = icon .. " " .. categoryName
    button.TextColor3 = CurrentTheme.Text
    button.TextSize = 16
    button.Font = Enum.Font.GothamBold
    button.Parent = CategoryPanel
    
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 8)
    btnCorner.Parent = button
    
    local highlight = Instance.new("Frame")
    highlight.Size = UDim2.new(0, 4, 1, 0)
    highlight.BackgroundColor3 = CurrentTheme.Accent
    highlight.BorderSizePixel = 0
    highlight.BackgroundTransparency = 1
    highlight.Parent = button
    
    button.MouseButton1Click:Connect(function()
        -- Clear content
        for _, child in pairs(ContentPanel:GetChildren()) do
            if child:IsA("Frame") then
                child:Destroy()
            end
        end
        
        -- Load commands for category
        local commands = AdminCommands[categoryName]
        if commands then
            for _, cmd in pairs(commands) do
                CreateCommandButton(cmd)
            end
        end
        
        -- Highlight animation
        highlight:TweenSize(UDim2.new(0, 4, 1, 0), "Out", "Quad", 0.3, true)
        TweenService:Create(highlight, TweenInfo.new(0.3), {BackgroundTransparency = 0}):Play()
    end)
    
    button.MouseEnter:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = CurrentTheme.Secondary}):Play()
    end)
    
    button.MouseLeave:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = CurrentTheme.Primary}):Play()
    end)
    
    return button
end

-- ‚ö° Create Command Button
local function CreateCommandButton(commandData)
    local cmdFrame = Instance.new("Frame")
    cmdFrame.Size = UDim2.new(0, 140, 0, 100)
    cmdFrame.BackgroundColor3 = CurrentTheme.Primary
    cmdFrame.BorderSizePixel = 0
    cmdFrame.Parent = ContentPanel
    
    local cmdCorner = Instance.new("UICorner")
    cmdCorner.CornerRadius = UDim.new(0, 10)
    cmdCorner.Parent = cmdFrame
    
    local iconLabel = Instance.new("TextLabel")
    iconLabel.Size = UDim2.new(1, 0, 0, 40)
    iconLabel.BackgroundTransparency = 1
    iconLabel.Text = commandData.icon
    iconLabel.TextSize = 32
    iconLabel.Parent = cmdFrame
    
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Size = UDim2.new(1, -10, 0, 25)
    nameLabel.Position = UDim2.new(0, 5, 0, 45)
    nameLabel.BackgroundTransparency = 1
    nameLabel.Text = commandData.name
    nameLabel.TextColor3 = CurrentTheme.Text
    nameLabel.TextSize = 14
    nameLabel.Font = Enum.Font.GothamBold
    nameLabel.TextWrapped = true
    nameLabel.Parent = cmdFrame
    
    local descLabel = Instance.new("TextLabel")
    descLabel.Size = UDim2.new(1, -10, 0, 20)
    descLabel.Position = UDim2.new(0, 5, 0, 72)
    descLabel.BackgroundTransparency = 1
    descLabel.Text = commandData.desc
    descLabel.TextColor3 = CurrentTheme.Text
    descLabel.TextSize = 10
    descLabel.Font = Enum.Font.Gotham
    descLabel.TextTransparency = 0.5
    descLabel.TextWrapped = true
    descLabel.Parent = cmdFrame
    
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(1, 0, 1, 0)
    button.BackgroundTransparency = 1
    button.Text = ""
    button.Parent = cmdFrame
    
    -- Hover effect
    button.MouseEnter:Connect(function()
        TweenService:Create(cmdFrame, TweenInfo.new(0.2), {
            BackgroundColor3 = CurrentTheme.Accent,
            Size = UDim2.new(0, 145, 0, 105)
        }):Play()
    end)
    
    button.MouseLeave:Connect(function()
        TweenService:Create(cmdFrame, TweenInfo.new(0.2), {
            BackgroundColor3 = CurrentTheme.Primary,
            Size = UDim2.new(0, 140, 0, 100)
        }):Play()
    end)
    
    -- Command execution
    button.MouseButton1Click:Connect(function()
        ExecuteCommand(commandData.name)
        
        -- Click animation
        local originalSize = cmdFrame.Size
        cmdFrame:TweenSize(UDim2.new(0, 135, 0, 95), "Out", "Quad", 0.1, true, function()
            cmdFrame:TweenSize(originalSize, "Out", "Elastic", 0.3, true)
        end)
    end)
end

-- üöÄ Command Execution System
function ExecuteCommand(commandName)
    local char = Player.Character
    if not char then return end
    
    -- Show notification
    CreateNotification("‚ö° Executing: " .. commandName)
    
    if commandName == "God Mode" then
        local humanoid = char:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.MaxHealth = math.huge
            humanoid.Health = math.huge
        end
        
    elseif commandName == "Fly" then
        -- Flying system
        local flying = false
        local speed = 50
        local bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.Velocity = Vector3.new(0, 0, 0)
        bodyVelocity.MaxForce = Vector3.new(0, 0, 0)
        bodyVelocity.Parent = char.HumanoidRootPart
        
        local bodyGyro = Instance.new("BodyGyro")
        bodyGyro.MaxTorque = Vector3.new(0, 0, 0)
        bodyGyro.Parent = char.HumanoidRootPart
        
        flying = not flying
        
        if flying then
            bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
            bodyGyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
        else
            bodyVelocity.MaxForce = Vector3.new(0, 0, 0)
            bodyGyro.MaxTorque = Vector3.new(0, 0, 0)
        end
        
    elseif commandName == "Speed" then
        local humanoid = char:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = 100
        end
        
    elseif commandName == "Invisible" then
        for _, part in pairs(char:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Transparency = 1
            end
        end
        
    elseif commandName == "Day" then
        game.Lighting.ClockTime = 12
        
    elseif commandName == "Night" then
        game.Lighting.ClockTime = 0
        
    elseif commandName == "Rainbow" then
        spawn(function()
            while wait(0.1) do
                for _, part in pairs(char:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.Color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                    end
                end
            end
        end)
        
    elseif commandName == "Fireworks" then
        for i = 1, 10 do
            local firework = Instance.new("Part")
            firework.Size = Vector3.new(1, 1, 1)
            firework.Position = char.HumanoidRootPart.Position + Vector3.new(math.random(-10, 10), 20, math.random(-10, 10))
            firework.Anchored = true
            firework.BrickColor = BrickColor.Random()
            firework.Material = Enum.Material.Neon
            firework.Shape = Enum.PartType.Ball
            firework.Parent = workspace
            
            local explosion = Instance.new("Explosion")
            explosion.Position = firework.Position
            explosion.BlastRadius = 10
            explosion.BlastPressure = 0
            explosion.Parent = workspace
            
            game.Debris:AddItem(firework, 2)
        end
        
    elseif commandName == "Sparkles" then
        local sparkles = Instance.new("Sparkles")
        sparkles.Parent = char.HumanoidRootPart
        
    elseif commandName == "Fire" then
        local fire = Instance.new("Fire")
        fire.Parent = char.HumanoidRootPart
        
    elseif commandName == "Reset" then
        char:BreakJoints()
    end
end

-- üí¨ Notification System
function CreateNotification(text)
    local notif = Instance.new("Frame")
    notif.Size = UDim2.new(0, 300, 0, 60)
    notif.Position = UDim2.new(1, 320, 0, 20)
    notif.BackgroundColor3 = CurrentTheme.Secondary
    notif.BorderSizePixel = 0
    notif.Parent = ScreenGui
    
    local notifCorner = Instance.new("UICorner")
    notifCorner.CornerRadius = UDim.new(0, 10)
    notifCorner.Parent = notif
    
    local notifText = Instance.new("TextLabel")
    notifText.Size = UDim2.new(1, -20, 1, 0)
    notifText.Position = UDim2.new(0, 10, 0, 0)
    notifText.BackgroundTransparency = 1
    notifText.Text = text
    notifText.TextColor3 = CurrentTheme.Text
    notifText.TextSize = 16
    notifText.Font = Enum.Font.GothamBold
    notifText.TextXAlignment = Enum.TextXAlignment.Left
    notifText.Parent = notif
    
    -- Slide in animation
    notif:TweenPosition(UDim2.new(1, -320, 0, 20), "Out", "Quad", 0.5, true)
    
    -- Auto remove
    wait(3)
    notif:TweenPosition(UDim2.new(1, 320, 0, 20), "Out", "Quad", 0.5, true)
    wait(0.5)
    notif:Destroy()
end

-- üéØ Initialize Categories
CreateCategoryButton("Player", "üë§", 1)
CreateCategoryButton("World", "üåç", 2)
CreateCategoryButton("Fun", "üéâ", 3)
CreateCategoryButton("Tools", "üîß", 4)
CreateCategoryButton("Server", "‚öôÔ∏è", 5)

-- üñ±Ô∏è Dragging System
local dragging = false
local dragInput
local dragStart
local startPos

local function update(input)
    local delta = input.Position - dragStart
    MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

TopBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

TopBar.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        update(input)
    end
end)

-- ‚ú® Opening Animation
MainFrame.Size = UDim2.new(0, 0, 0, 0)
MainFrame.Rotation = 180
MainFrame:TweenSize(UDim2.new(0, 650, 0, 450), "Out", "Elastic", 1, true)
TweenService:Create(MainFrame, TweenInfo.new(1, Enum.EasingStyle.Elastic), {Rotation = 0}):Play()

-- üéä Welcome notification
wait(1)
CreateNotification("üéâ Welcome to Admin Universe!")
wait(0.5)
CreateNotification("üí´ Script by Claude AI")

print("‚úÖ Admin Universe loaded successfully!")
